package Vida;
import java.util.Random;
public class ExDeLaVida {

	public static void main(String[] args) {
	Random ran = new Random();	
	int[][] m = new int[10][10];
	
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 10; j++) {
          m[i][j] = ran.nextInt(10);
		System.out.println(m[i][j] + " ");
		}
		
	}
		}
}
