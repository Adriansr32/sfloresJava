package Vida;
import java.util.Random;
public class ExDeLaVida {

	public static void main(String[] args) {
	Random ran = new Random();	
	int[][] m = new int[3][3];
	
	for(int i = 0; i < 3; i++) {
		for(int j = 0; j < 3; i++) {
			m[i][j] = ran.nextInt(1);
		System.out.println(m[i][j] + " ");
		}
		
	}
		}
}
