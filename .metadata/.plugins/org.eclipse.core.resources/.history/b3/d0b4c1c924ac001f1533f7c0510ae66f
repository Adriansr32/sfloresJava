package Vida;
import java.util.Random;
public class ExDeLaVida {

	public static void main(String[] args) {
	Random ran = new Random();	
	int[][] m = new int[3][3];
	
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
          m[i][j] = ran.nextInt(2);
		System.out.print(m[i][j] + " ");
		}
		
	}
		}
}
